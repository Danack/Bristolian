{
    "name": "alwaysseptember/open-docs",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
        "danack/rfc-codex": "dev-master",
        "danack/configurator": "1.1.1",
        "danack/console": "^2.6",
        "danack/esprintf": "^2.0.0",
        "danack/looping-exec": "^0.3.0",
        "danack/json-safe": "^0.1.1",
        "danack/slim4-auryn-invoker": "dev-dev",
        "danack/typespec": "0.1.0",
        "dmore/chrome-mink-driver": "2.8.1",
        "ext-redis": "*",
        "justinrainbow/json-schema": "^5.2.7",
        "ramsey/uuid": "4.0.1",
        "rdlowrey/auryn": "^1.4.2",
        "seld/jsonlint": "^1.7",
        "slim/slim": "4.10.0",
        "tholu/php-cidr-match": "^0.2.0",
        "danack/aurynconfig": "^0.9.1",
        "ext-json": "*",
        "ext-curl": "*",
        "michelf/php-markdown": "^1.9",
        "thecodingmachine/safe": "^1.3",
        "league/commonmark": "^2.3.5",
        "ext-posix": "*",
        "spatie/yaml-front-matter": "^2.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behat/mink-extension": "^2.3",
        "danack/coding-standard": "0.3.0",
        "dmore/behat-chrome-extension": "^1.4.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "9.5.6",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "slevomat/coding-standard": "^7.2.1",
        "squizlabs/php_codesniffer": "^3.7.0",
        "yoast/yoastcs": "1.0",
        "friends-of-behat/mink-extension": "^2.5",
        "behat/mink-browserkit-driver": "^1.3"
    },
    "autoload": {
        "psr-0": {
            "Bristolian\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "BristolianTest\\": "test/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0"
        },
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
