{
    "name": "danack/bristolian",
    "type": "project",

    "repositories": {
        "danack_injector": {
            "type": "vcs",
            "url": "git@github.com:danack/Injector.git"
        }
    },
    "require": {
        "danack/configurator": "1.1.1",
        "danack/console": "^2.6.3",
        "danack/esprintf": "^2.0.0",
        "danack/injector": "0.9.0",
        "danack/looping-exec": "^0.3.0",
        "danack/php-unit-helper": "^0.5.0",
        "danack/json-safe": "^0.1.1",
        "danack/slim-dispatcher": "dev-dev",
        "danack/datatype": "0.7.0",
        "dmore/behat-chrome-extension": "1.4.1",
        "dmore/chrome-mink-driver": "2.10.0",
        "hashids/hashids": "5.0.2",
        "ext-redis": "*",
        "justinrainbow/json-schema": "^5.2.7",
        "laminas/laminas-diactoros":"^2.24.0",
        "ramsey/uuid": "^4.7",
        "predis/predis": "^v2.3.0",
        "seld/jsonlint": "^1.7",
        "slim/slim": "4.10.0",
        "tholu/php-cidr-match": "^0.2.0",
        "ext-json": "*",
        "ext-curl": "*",
        "michelf/php-markdown": "^1.9",
        "thecodingmachine/safe": "^1.3",
        "league/commonmark": "^2.4.0",
        "ext-posix": "*",
        "spatie/yaml-front-matter": "^2.0",
        "ext-pdo": "*",
        "chillerlan/php-qrcode": "^4.3",
        "minishlink/web-push": "^8.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "sendgrid/sendgrid": "^8.1",
        "mailgun/mailgun-php": "^4.3",
        "symfony/http-client": "^7.1",
        "nyholm/psr7": "^1.8",
        "ext-xmlreader": "*",
        "auroras-live/suncalc-php": "dev-master",
        "psr/http-server-middleware": "^1.0.2",
        "php" : "^8.2",
        "ircmaxell/random-lib": "^1.1.0",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-exif": "*",
        "ext-libxml": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "behat/behat": "^3.0",
        "friends-of-behat/mink-extension": "^2.0",
        "danack/coding-standard": "0.3.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "11.5.48",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "slevomat/coding-standard": "^7.2.1",
        "squizlabs/php_codesniffer": "^3.7.0",
        "yoast/yoastcs": "1.0",
        "qossmic/deptrac-shim": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Asm\\": "asm_in_project/lib/",
            "Bristolian\\": "src/",
            "BristolianBehat\\": "src/",
            "Key\\": "src/",
            "OpenApi\\": "src/",
            "UrlFetcher\\": "src/"
        },
        "psr-4": {
            "Bristolian\\Database\\": "src/Bristolian/Database"
        },
        "files": [
            "src/functions.php",
            "src/functions_common.php",
            "src/functions_exif.php",
            "src/functions_tinned_fish.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "BristolianTest\\": "test/",
            "IntegrationTest\\": "test/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
